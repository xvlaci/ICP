####### Compile

main.o: src/main.cpp src/mainwindow.h \
		src/controller.h \
		src/board.h \
		src/square.h \
		src/character.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o main.o src/main.cpp

mainwindow.o: src/mainwindow.cpp src/mainwindow.h \
		ui_mainwindow.h \
		src/serverwindow.h \
		src/clientwindow.h \
		src/client.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o mainwindow.o src/mainwindow.cpp

server.o: src/server.cpp src/server.h \
		src/controller.h \
		src/board.h \
		src/square.h \
		src/character.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o server.o src/server.cpp

client.o: src/client.cpp src/client.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o client.o src/client.cpp

character.o: src/character.cpp src/character.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o character.o src/character.cpp

square.o: src/square.cpp src/square.h \
		src/character.h
