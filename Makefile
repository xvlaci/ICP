	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o clientwindow.o src/clientwindow.cpp

connectdialog.o: src/connectdialog.cpp src/connectdialog.h \
		ui_connectdialog.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o connectdialog.o src/connectdialog.cpp

controller.o: src/controller.cpp src/controller.h \
		src/board.h \
		src/square.h \
		src/character.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o controller.o src/controller.cpp

